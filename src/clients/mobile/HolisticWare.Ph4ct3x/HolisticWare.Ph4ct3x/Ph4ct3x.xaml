<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="HolisticWare.Ph4ct3x.Ph4ct3x">
	<ContentPage.Content>

	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="Auto"></RowDefinition>
			<RowDefinition Height="Auto"></RowDefinition>
			<RowDefinition Height="Auto"></RowDefinition>
			<RowDefinition Height="Auto"></RowDefinition>
			<RowDefinition Height="Auto"></RowDefinition>
		</Grid.RowDefinitions>

		<Button
			x:Name="buttonStopWatch"
			Grid.Row="0" Grid.Columns="0"
			Text="StopWatch"
			>
		</Button>
		<Button
			x:Name="buttonTracker"
			Grid.Row="1" Grid.Columns="0"
			Text="Tracker"
			>
		</Button>
		<Button
			x:Name="buttonSettings"
			Grid.Row="2" Grid.Columns="0"
			Text="Settings"
			>
		</Button>
	</Grid>
	</ContentPage.Content>
</ContentPage>
